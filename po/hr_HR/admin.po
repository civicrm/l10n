msgstr ""
"Da biste uklonili privremene datoteke za prijenos (građanska mapa/mapa za "
"prijenos), dodajte <em>%1</em>"

#: templates/CRM/Admin/Form/Setting/Debugging.hlp
msgid "To cleanup both, add <em>%1</em>"
msgstr "Da biste očistili oba, dodajte <em>%1</em>"

#: templates/CRM/Admin/Form/Setting/Debugging.hlp
msgid "Stack Trace"
msgstr "Praćenje stoga"

#: templates/CRM/Admin/Form/Setting/Debugging.hlp
