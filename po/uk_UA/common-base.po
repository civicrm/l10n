msgid "Add/overwrite?"
msgstr ""

#: templates/CRM/Contact/Form/Merge.tpl
msgid "overwrite"
msgstr "перезаписати"

#: templates/CRM/Contact/Form/Merge.tpl
msgid "match"
msgstr "співпадіння"

#: templates/CRM/Contact/Form/Merge.tpl
msgid "Move related..."
msgstr "Перенести пов'язане"

#: templates/CRM/Contact/Form/Merge.tpl
