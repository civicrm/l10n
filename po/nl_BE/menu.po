msgctxt "menu"
msgid "Mailing Summary"
msgstr "Overzicht mailings"

#: xml/templates/civicrm_navigation.tpl
msgctxt "menu"
msgid "Summary statistics for mailings"
msgstr "Statistiekenoverzicht voor mailings"

#: xml/templates/civicrm_navigation.tpl
msgctxt "menu"
msgid "Mail Opened"
msgstr "Geopende e-mails"

#: xml/templates/civicrm_navigation.tpl
msgctxt "menu"
msgid "Display contacts who opened emails from a mailing"
msgstr "Toont de contacten die de e-mail van de mailing hebben geopend"

