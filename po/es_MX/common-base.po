msgstr "sobreescribir"

#: templates/CRM/Contact/Form/Merge.tpl
msgid "match"
msgstr "coincidencia"

#: templates/CRM/Contact/Form/Merge.tpl
msgid "Move related..."
msgstr "Move relacionado..."

#: templates/CRM/Contact/Form/Merge.tpl
msgid "migrate"
msgstr "migrar"

