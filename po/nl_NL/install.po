msgid "Drupal Database Settings"
msgstr "Drupal database-instellingen"

#: install/template.html
msgid "Other Settings"
msgstr "Overige instellingen"

#: install/template.html
msgid "Re-check requirements"
msgstr "Vereisten opnieuw controleren"

#: install/template.html
msgid "CiviCRM Database Details"
msgstr "CiviCRM databasedetails"

#: install/template.html
msgid "Drupal Database Details"
msgstr "Drupal databasedetails"

#: install/template.html
msgid "Requirements"
msgstr "Vereisten"
